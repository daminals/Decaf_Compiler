class BankAccount
{
    public float balance;
    public string firstName;
    public string lastName;
    
    public BankAccount(string fn, string ln)
    {
        this.firstName = fn;
        this.lastName = ln;
        this.balance = 0.0;
    }
    
    public float deposit(float amount)
    {
        this.balance = this.balance + amount;
        return this.balance;
    }
    
    public float withdrawal(float amount)
    {
        if (this.balance > amount)
        {
            this.balance = this.balance - amount;
        }
        else
        {
            Out.print("Insufficient funds in account.");
        }
        return this.balance;
    }
    
    public float get_balance()
    {
        return this.balance;
    }
}

class CheckingAccount extends BankAccount
{
    public int checkNumber;
    
    public CheckingAccount(string fn, string ln)
    {
        this.firstName = fn;
        this.lastName = ln;
        this.balance = 0.0;
        this.checkNumber = 0;
    }
    
    public int debit(float amount)
    {
        int ccn;
        
        ccn = this.checkNumber;
        if (amount < this.balance)
        {
            this.checkNumber++;
            this.balance = this.balance - amount;
        }
        else
        {
            Out.print("Insufficient funds in account.");
        }
        return ccn;
    }
}

